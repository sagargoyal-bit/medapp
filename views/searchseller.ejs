<!DOCTYPE html>
<html>
  <head>
    <%- include('header.ejs') -%>
  </head>
  <body>
    
    
      <section class="body">

        <!-- start: header -->
       <!-- end: header -->
  
        <div class="inner-wrapper">
          <!-- start: sidebar -->
          <aside id="sidebar-left" class="sidebar-left">
          
            <div class="sidebar-header">
             
              <div class="sidebar-toggle hidden-xs" data-toggle-class="sidebar-left-collapsed" data-target="html" data-fire-event="sidebar-left-toggle">
                <i class="fa fa-bars" aria-label="Toggle sidebar"></i>
              </div>
            </div>
          
            <div class="nano">
              <div class="nano-content">
              <!--  navbar-->
               <%- include('navbar.ejs')-%>
               
                <hr class="separator" />
          
               
          
                <hr class="separator" />
          
              </div>
          
            </div>
          
          </aside>
          
          <!-- table -->
  
          <section role="main" class="content-body">
            <header class="page-header">
              <h2>Dashboard</h2>
            </header>
            
  
              <div class="container">
                <div class="col-lg-12">
                  <section class="panel">
                    <header class="panel-heading">
                      <div class="panel-actions">
                        <a href="#" class="fa fa-caret-down"></a>
                        <a href="#" class="fa fa-times"></a>
                      </div>
    
                      <h2 class="panel-title">new Form</h2>
                      
                    </header>
                    <div class="panel-body">
                      <form class="form-inline" action="/search/" method="POST">
                        <div class="form-group">
                          <label class="sr-only" for="exampleInputUsername2">Medicine</label>
                          <input type="text" class="form-control" id="exampleInputUsername2" name="medicine" placeholder="Medicine">
                        </div>
                        <div class="form-group">
                          <label class="sr-only" for="exampleInputPassword2">batchnumber</label>
                          <input type="text" class="form-control" id="exampleInputPassword2" name="bnumber" placeholder="batchnumber">
                        </div>
    
                     
    
                        <input type="submit" class="btn btn-info" value="search">
                       
                      </form>
                    </div>
                  </section>
                </div>
                </div>
               
                <hr>
                <hr>
            
               
                  
             <div class="row">
                <div class="col-md-10">
                    <section class="panel">
                      <header class="panel-heading">
                        <div class="panel-actions">
                          <a href="#" class="fa fa-caret-down"></a>
                          
                        </div>
                
                        <h1 class="panel-title">Sellers</h1>
                      </header>
                      <div class="panel-body">
                        <div class="table-responsive">
                          <table class="table table-hover mb-none" id="tid">
                            <thead>
                              <tr>
                                
                                <th>#</th>
                                <th>Medicine name</th>
                                <th>Seller name</th>
                                <th>Batch number</th>
                                <th>Expiry month</th>
                              </tr>
                            </thead>
                            
                            <tbody>
                              <% if (recordseller.length >0 ){ %>
                                <% recordseller.forEach(function(row) { %>
                              <tr>
                                <th>#</th>
                                <td><%= row.medicine %></td>
                                <td><%= row.sellername %></td>
                                <td><%= row.batchnumber %></td>
                                <td><%= row.expirymonth %></td> 
                              </tr>
                             
                            <%})}else { %>
                              <th scope="colspan=5">NO data found</th>
                            <%}%> 
                            </tbody>
                            
                          </table>
                        </div>
                      </div>
                    </section>
                  </div>
            </div>
            
            <!-- start: page -->
  
  
            
  
            <!-- end: page -->
             </section>
    </div>
  
      </section>
  <!--  footer -->
  <%- include('footer.ejs') -%>
  <script>
   $(document).ready(function() {
     $('#tid').DataTable();
   } );
   </script>
    </body>
  </html>


  
